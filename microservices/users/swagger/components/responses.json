{
    "components": {
        "responses": {
            "Ok": {
                "description": "Ok"
            },
            "Unauthorized": {
                "description": "Unauthorized",
                "content": {
                   "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Error"
                        },
                        "example": {
                            "code": 401,
                            "message": "Unauthorized user"
                        }
                    }
                }
            },
            "NotFound": {
                "description": "The specified resource was not found",
                "content": {
                   "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Error"
                        },
                        "example": {
                            "code": 404,
                            "message": "Object not found"
                        }
                    }
                }
            },
            "UnexpectedError": {
                "description": "Unexpected error",
                "content": {
                   "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Error"
                        }
                    }
                }
            }
        },
        "schemas": {
            "Error": {
                "type": "object",
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        }
    }
}